<component name="QrCodePoster" extends="Poster">
    <interface>
        <field id="text" type="string" onChange="OnTextChange" />
        <field id="padding" type="integer" />
        <field id="isBusy" type="boolean" onChange="OnIsBusy" alwaysNotify="true" />
    </interface>
    <children>
        <BusySpinner
            id="busySpinner"
            spinInterval="1"
            visible="false" />
    </children>
</component>