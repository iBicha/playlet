        <field id="enabled" type="boolean" />
        <field id="applicationInfo" type="node" />
        <field id="preferences" type="node" />
        <field id="LogInfo" type="string" alwaysNotify="true" />
        <field id="LogWarn" type="string" alwaysNotify="true" />
        <field id="LogError" type="string" alwaysNotify="true" />
    </interface>
</component>