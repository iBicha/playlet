<component name="ChannelContentNode" extends="ContentNode">
    <interface>
        <function name="LoadChannel" />

        <field id="type" type="string" value="channel" />
        <field id="loadState" type="string" />
        <!-- index in a feed -->
        <field id="feedSourcesIndex" type="integer" value="-1" />
        <field id="HDItemWidth" type="float" value="196" />
        <!-- NOTE: "_author" not "author". See PlaylistContentNode.xml for explanation -->
        <field id="_author" type="string" />
        <field id="authorId" type="string" />
        <field id="banner" type="string" />
        <field id="channelHandle" type="string" />
        <field id="description" type="string" />
        <field id="subCount" type="integer" />
        <field id="subCountText" type="string" />
        <field id="tabs" type="array" />
        <field id="tabsParams" type="assocarray" />
        <field id="thumbnail" type="string" />
        <field id="videoCount" type="integer" />
        <field id="videoCountText" type="string" />
    </interface>
</component>