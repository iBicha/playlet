<component name="JobCallback" extends="Node">
    <interface>
        <field id="done" type="boolean" />
        <field id="success" type="boolean" />
        <field id="error" type="string" />
        <field id="data" type="assocarray" />
        <!-- read/write field, can be used to cancel a job, and to check for cancellation. -->
        <field id="cancel" type="boolean" />
    </interface>
</component>